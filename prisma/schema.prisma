generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UAT {
  id                  Int       @id @default(autoincrement())
  customer_id         String
  acct_id             String
  product             String
  sales_method        String
  Acct_Type_Desc      String
  SKU                 String
  Service_Stat        String
  customer_category   String
  credit_class        String
  str_addr            String
  city                String
  state               String
  zip_code            String
  auto_payment_method String
  email               String    
  cell_phone          String
  bre_start_dt        String
  bre_end_dt          String?
  lease_device        String
  acct_status         String
  cycle               String
}
